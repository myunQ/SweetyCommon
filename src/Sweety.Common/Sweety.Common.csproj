<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<VersionPrefix>1.0.0</VersionPrefix>
	<VersionSuffix>alpha</VersionSuffix>
    <TargetFrameworks>net6.0;netstandard2.1;netstandard2.0</TargetFrameworks>
    
    <Authors>Mingyun Qin</Authors>
    <Copyright>Mingyun Qin</Copyright>
    <Owners>Mingyun Qin</Owners>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>- 有助于快速开发项目的实用工具组件。
- Utility components to help develop projects quickly.</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <SignAssembly>True</SignAssembly>
    <PackageProjectUrl>https://github.com/myunq/sweetycommon</PackageProjectUrl>
    <RepositoryUrl>https://github.com/myunq/sweetycommon</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>- 对数据库操作进行了大量的修改。</PackageReleaseNotes>
	<PackageReadmeFile>README.md</PackageReadmeFile>
    <AssemblyOriginatorKeyFile>..\..\SweetySignKeyPair.snk</AssemblyOriginatorKeyFile>
    <PackageTags>sweety</PackageTags>
	<AssemblyVersion>$(VersionPrefix).*</AssemblyVersion>
	<GenerateAssemblyFileVersionAttribute>False</GenerateAssemblyFileVersionAttribute>
	<Deterministic>False</Deterministic>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(TargetFramework)' != 'netstandard2.0' ">
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType></DebugType>
    <DocumentationFile>bin\Release\netstandard2.0\Sweety.Common.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
	<InternalsVisibleTo Include="$(MSBuildProjectName).DataProvider.PostgreSQL" PublicKey="" />
	<InternalsVisibleTo Include="$(MSBuildProjectName).DataProvider.SqlServer" PublicKey="" />
	<InternalsVisibleTo Include="$(MSBuildProjectName).DataProvider.MySql" PublicKey="" />
	<InternalsVisibleTo Include="$(MSBuildProjectName).DataProvider.SQLite" PublicKey="" />
	<InternalsVisibleTo Include="$(MSBuildProjectName).Caching.Redis" PublicKey="" />

	<InternalsVisibleTo Include="$(MSBuildProjectName).Tests" PublicKey="" />
  </ItemGroup>
	
  <ItemGroup>
    <Folder Include="Cryptography\" />
    <Folder Include="Extensions\" />
    <Folder Include="Comparer\" />
    <Folder Include="Security\" />
    <Folder Include="Caching\" />
    <Folder Include="Compression\" />
    <Folder Include="Net\" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Properties\Localization.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Localization.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Properties\Localization.Designer.cs">
      <DependentUpon>Localization.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'netstandard2.0' ">
    <PackageReference Include="System.Buffers" Version="4.5.1" />
  </ItemGroup>

  <ItemGroup>
    <None Include="README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
</Project>
